{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    //"esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true
  }
}
